{
  "name": "s3-zip-handler",
  "version": "1.0.0",
  "description": "extract a zip file provided from your AWS s3 bucket to any other place",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TindinOficial/s3-zip-handler.git"},
  "scripts": {
    "lint": "eslint .",
    "test": "jest --passWithNoTests --maxWorkers=50%",
    "test:watch": "npm test -- --watch ",
    "test:staged": "npm test --",
    "test:coverage": "npm test -- --coverage",
    "build": "tsc",
    "prepare" : "npm run build",
    "prepublishOnly" : "npm test && npm run lint",
    "preversion": "npm run lint",
    "version" : "git add -A src",
    "postversion" : "git push && git push --tags"
  },
  "keywords": [
    "s3",
    "zip",
    "unzip",
    "unzipper",
    "decompress",
    "unzip-s3",
    "s3-zip",
    "s3-unzipper",
    "s3-unzip"
  ],
  "author": "Tindin",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^28.1.3",
    "@types/node": "^18.0.0",
    "@types/unzipper": "^0.10.5",
    "@typescript-eslint/eslint-plugin": "^5.30.3",
    "@typescript-eslint/parser": "^5.30.3",
    "eslint": "^8.18.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^28.1.2",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.2.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.7.4",
    "aws-sdk-mock": "^5.6.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.120.0",
    "aws-sdk": "^2.1094.0",
    "unzipper": "^0.10.11"
  }
}
